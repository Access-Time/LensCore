{
  "name": "@accesstime/lenscore",
  "version": "0.1.35",
  "description": "Open-source accessibility testing and web crawling platform",
  "main": "dist/index.js",
  "bin": {
    "lens-core": "bin/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:cli": "tsc -p tsconfig.cli.json",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "accessibility",
    "testing",
    "web-crawling",
    "axe-core",
    "docker"
  ],
  "author": "AccessTime",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/ioredis": "^4.28.10",
    "aws-sdk": "^2.1490.0",
    "axe-core": "^4.8.2",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "inquirer": "^12.10.0",
    "ioredis": "^5.8.1",
    "openai": "^6.5.0",
    "ora": "^5.4.1",
    "puppeteer": "^24.15.0",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "commonjs"
}
